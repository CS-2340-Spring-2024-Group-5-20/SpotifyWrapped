        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SpotifyWrappedInstagramGroup5"
        tools:targetApi="31">
        <activity
            android:name=".SpotifyWrappedLastPageFestival"/>
        <activity android:name=
            ".SpotifyWrappedGenrePage"/>
        <activity
            android:name=".SpotifyWrappedInitialPage"/>
        <activity
            android:name=".CommentPage"
            android:windowSoftInputMode="adjustResize"/>
        <activity
            android:name=".ProfilePageSearchPopOut"
            />
        <activity
            android:name=".SpotifyWrappedPopUp"
            />
        <activity
            android:name=".SignoutPage"
            android:exported="false" />
        <activity
            android:name=".ForgotPassword"
            android:exported="false" />
        <activity
            android:name=".EditProfile"
            android:exported="false" />
        <activity android:name=".SearchPage" />
        <activity android:name=".HomePage" />
        <activity android:name=".MyWraps" />
        <activity android:name=".PostPage" />
