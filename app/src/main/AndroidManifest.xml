            android:name=".ProfilePage"
            android:exported="false" />
        <activity
            android:name=".Logout"
            android:exported="false" />
        <activity
            android:name=".SpotifyLogin"
            android:exported="false">
            <intent-filter>
                <data
                    android:host="auth"
                    android:scheme="spotifywrappedinstagramgroup5" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Login"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Register"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.SpotifyWrappedInstagramGroup5">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <intent-filter>
                <data
                    android:host="auth"
                    android:scheme="spotifywrappedinstagramgroup5" />
            </intent-filter>
        </activity>
    </application>

</manifest>